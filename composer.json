{
  "name": "terramar/sales-bundle",
  "type": "symfony-bundle",
  "repositories": [
    {
      "type": "git",
      "url": "git@terramarlabs.com:OrkestraReportBundle.git"
    },
    {
      "type": "git",
      "url": "git@terramarlabs.com:OrkestraPdfBundle.git"
    },
    {
      "type": "git",
      "url": "git@terramarlabs.com:TerramarCustomerBundle.git"
    },
    {
      "type": "git",
      "url": "git@terramarlabs.com:TerramarNotificationBundle.git"
    }
  ],
  "require": {
    "php": ">=5.3.0",
    "orkestra/common": "1.0.*",
    "orkestra/application-bundle": "dev-develop",
    "orkestra/report-bundle": "dev-develop",
    "orkestra/pdf-bundle": "dev-master",
    "terramar/customer-bundle": "1.0.*",
    "tcpdf/tcpdf": "dev-master"
  },
  "suggest": {
    "terramar/notification-bundle": "For intra and inter-office messaging"
  },
  "autoload": {
    "psr-0": { "Terramar\\Bundle\\SalesBundle": "" }
  },
  "target-dir": "Terramar/Bundle/SalesBundle"
}
